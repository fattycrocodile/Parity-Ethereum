use ipc::*;

#[derive(Binary)]
enum Root {
	Top,
	Middle(u32, u64),
	Bottom {
		number1: u32,
		number2: u64,
	},
}

#[derive(Binary)]
struct DoubleRoot {
	x1: u32,
	x2: u64,
	x3: u32,
}
